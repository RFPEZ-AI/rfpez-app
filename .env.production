# Production Environment Variables for Azure Static Web Apps
# Copy your values from .env.local and update the Auth0 callback URLs

# Auth0 Configuration
REACT_APP_AUTH0_DOMAIN=your-domain.auth0.com
REACT_APP_AUTH0_CLIENT_ID=your-client-id

# Supabase Configuration  
REACT_APP_SUPABASE_URL=your-supabase-url
REACT_APP_SUPABASE_ANON_KEY=your-supabase-anon-key

# Production URL (update after deployment)
REACT_APP_PRODUCTION_URL=https://your-app-name.azurestaticapps.net
