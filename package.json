    "start": "cross-env GENERATE_SOURCEMAP=false DISABLE_ESLINT_PLUGIN=true react-scripts start",
    "start:api": "node api-server/index.js",
    "start:dev": "concurrently \"npm run start:api\" \"npm start\"",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "lint": "eslint src/**/*.{js,jsx,ts,tsx} --max-warnings=0",
    "lint:fix": "eslint src/**/*.{js,jsx,ts,tsx} --fix",
    "mcp:deploy": "bash deploy-mcp.sh",
    "mcp:test": "node test-mcp.js",
    "mcp:install": "npm install node-fetch@2.6.7",
    "claude-api:deploy": "supabase functions deploy claude-api-v3",
    "claude-api:test": "node test-v3-browser-integration.js",
    "version:patch": "npm version patch && git push origin master --follow-tags",
    "version:minor": "npm version minor && git push origin master --follow-tags",
    "version:major": "npm version major && git push origin master --follow-tags",
    "build:deploy": "npm run build && git add . && git commit -m \"Deploy build $(date)\" && git push origin master"
  },
  "browserslist": [
    "Chrome >=79",
    "ChromeAndroid >=79",
    "Firefox >=70",
    "Edge >=79",
    "Safari >=14",
    "iOS >=14"
  ],
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!(@ionic/react|@ionic/react-router|@ionic/core|@stencil/core|ionicons)/)"
    ],
    "moduleNameMapper": {
      "^react-markdown$": "<rootDir>/src/__mocks__/react-markdown.tsx",
      "^remark-gfm$": "<rootDir>/src/__mocks__/remark-gfm.ts"
    },
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ]
  },
